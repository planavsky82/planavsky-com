    "start:api": "ts-node -P ./projects/api/server.tsconfig.json ./projects/api/server.ts",
    "start:ul-demo": "ng serve ultimate-list-demo",
    "start:mffr": "ng serve mffr --proxy-config ./proxy.json",
    "start:mffr:mobile": "ionic serve --project=mffr -l --open",
    "start:mffr:cordova": "ng build mffr --prod --base-href . --output-path ./CordovaMobileApp/www/ && cd CordovaMobileApp && cordova platform add ios || true && cordova emulate ios",
