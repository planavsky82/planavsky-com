    "build:planavsky": "ng build --prod ultimate-list && ng build planavsky-com --prod",
    "add:component": "function add() { ng g module $1 --project=ultimate-list && ng g component $1 --project=ultimate-list; }; add",
    "firebase:deploy": "npm run build:mffr && firebase deploy",
    "firebase:login": "firebase login",
    "firebase:setup": "npm i -g firebase-tools && firebase init",
    "firebase:serve:api": "cd functions && npm run build && firebase serve",
    "firebase:deploy:api": "cd functions && npm run build && firebase deploy",
