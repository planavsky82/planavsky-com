        }
      }
    },
    "ultimate-list-demo-e2e": {
      "root": "projects/ultimate-list-demo-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
