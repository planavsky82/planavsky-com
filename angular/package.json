  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:api": "ts-node -P ./projects/api/server.tsconfig.json ./projects/api/server.ts",
    "start:ul-demo": "ng serve ultimate-list-demo",
    "start:mffr": "ng serve mffr --proxy-config ./proxy.json",
    "start:mffr:mobile": "ionic serve --project=mffr -l --open",
    "start:mffr:cordova": "ng build mffr --prod --base-href . --output-path ./CordovaMobileApp/www/ && cd CordovaMobileApp && cordova platform add ios || true && cordova emulate ios",
    "build:mffr:cordova": "cordova build ios --device --release --buildConfig build.json",
    "build": "ng build --prod ultimate-list",
    "test": "ng test ultimate-list --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:mffr": "ng build mffr --prod",
    "build:ul-demo": "ng build ultimate-list-demo --prod --base-href ./",
    "build:planavsky": "ng build --prod ultimate-list && ng build planavsky-com --prod",
    "add:component": "function add() { ng g module $1 --project=ultimate-list && ng g component $1 --project=ultimate-list; }; add",
    "firebase:deploy": "npm run build:mffr && firebase deploy",
    "firebase:login": "firebase login",
    "firebase:setup": "npm i -g firebase-tools && firebase init",
    "firebase:serve:api": "cd functions && npm run build && firebase serve",
    "firebase:deploy:api": "cd functions && npm run build && firebase deploy",
    "firebase:set:db:var": "export GOOGLE_APPLICATION_CREDENTIALS=\"./planavsky-com-7dafdf5fd67c.json\""
